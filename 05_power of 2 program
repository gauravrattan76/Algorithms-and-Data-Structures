function powerOf(n)
{
    if(n<=1) return false
    while(n>1)
        {
            let r = n%2
            if(r !== 0) return false
            n = n/2
        }
    return true;
}

console.log(powerOf(2))
console.log(powerOf(1))
console.log(powerOf(4))
console.log(powerOf(6))
console.log(powerOf(8))


Time complexity is O(n)


